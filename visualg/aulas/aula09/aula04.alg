algoritmo "semnome"

var
   C,Q:inteiro
   R,D: Real
inicio
    C <- 1
    Escreva("Quantas vezes voce quer converter?")
    Leia(Q)
    Enquanto (C <= 4)faca
             Escreval("Qual o valor em R$ ")
             Leia(R)
             D<- R/2.20
             Escreval("O valor convertido em US$", D:5:2)
    FimEnquanto
fimalgoritmo
